@{
	ViewData["Title"] = "Favorites";
}

<body>
	<div class="container">
		<div class="site-content-warp" style="padding-left: 0;">
			<!-- Page start -->
			<div class="page-section portfolio-page">
				<div class="portfolio-section">
					<div class="row">
						@for (int i = 0; i < 20; i++)
						{
							<div class="col-md-4">
								<div class="portfolio-item">
									<img src="img/portfolio/@(i % 3 + 2).jpg" alt="#">
									<div class="pi-info">
										<h5>Ice Cold Love</h5>
										<p>Nature Photography</p>
										<a href="#"><img src="img/arrow-right.png" alt=""></a>
									</div>
								</div>
							</div>
						}
					</div>
				</div>
				<!-- Page end -->
			</div>
		</div>
	</div>
</body>
